{
    "title": "Web Summary (Studio)",
    "description": "The following dashboard shows a general overview of the web.",
    "inputs": {
        "input_QmBwPYru": {
            "options": {
                "defaultValue": "*",
                "items": [
                    {
                        "label": "All",
                        "value": "*"
                    },
                    {
                        "label": "Get",
                        "value": "get"
                    },
                    {
                        "label": "Post",
                        "value": "post"
                    }
                ],
                "token": "method"
            },
            "title": "Method",
            "type": "input.dropdown"
        },
        "input_V1PTFrEd": {
            "context": {
                "formattedConfig": {
                    "number": {
                        "prefix": ""
                    }
                },
                "formattedStatics": ">statics | formatByType(formattedConfig)",
                "label": ">primary | seriesByName(\"status\") | renameSeries(\"label\") | formatByType(formattedConfig)",
                "statics": [
                    [
                        "All"
                    ],
                    [
                        "*"
                    ]
                ],
                "value": ">primary | seriesByName(\"status\") | renameSeries(\"value\") | formatByType(formattedConfig)"
            },
            "dataSources": {
                "primary": "ds_xEyrsihR"
            },
            "options": {
                "defaultValue": [
                    "*"
                ],
                "items": ">frame(label, value) | prepend(formattedStatics) | objects()",
                "token": "http_status"
            },
            "title": "HTTP Status",
            "type": "input.multiselect"
        },
        "input_sqH3eajk": {
            "options": {
                "defaultValue": "0,",
                "token": "time"
            },
            "title": "Time Range",
            "type": "input.timerange"
        }
    },
    "defaults": {
        "dataSources": {
            "ds.search": {
                "options": {
                    "queryParameters": {}
                }
            }
        },
        "visualizations": {
            "global": {
                "showProgressBar": true
            }
        }
    },
    "visualizations": {
        "viz_HejENik3": {
            "dataSources": {
                "primary": "ds_RUm6CDNi"
            },
            "options": {
                "legendTruncation": "ellipsisMiddle",
                "showRoundedY2AxisLabels": false,
                "showY2MajorGridLines": true,
                "y2AxisAbbreviation": "off",
                "yAxisAbbreviation": "off"
            },
            "showProgressBar": true,
            "title": "HTTP Status Codes by time",
            "type": "splunk.line"
        },
        "viz_Obx2jwh9": {
            "dataSources": {
                "primary": "ds_LsBC0DxN"
            },
            "options": {},
            "title": "Successful Transaction Requests by Eastern US Cities",
            "type": "splunk.pie"
        },
        "viz_YndmBpcv": {
