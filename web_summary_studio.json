                "primary": "ds_43sSJSXt"
            },
            "description": "**Filters applied**",
            "title": "Total Transaction Request from Western USA",
            "type": "splunk.singlevalue"
        },
        "viz_yC1X8oXB": {
            "dataSources": {
                "primary": "ds_rYDhnbyn"
            },
            "options": {
                "legendTruncation": "ellipsisMiddle",
                "showRoundedY2AxisLabels": false,
                "showY2MajorGridLines": true,
                "y2AxisAbbreviation": "off",
                "yAxisAbbreviation": "off"
            },
            "title": "HTTP Methods by time",
            "type": "splunk.line"
        }
    },
    "dataSources": {
        "ds_43sSJSXt": {
            "name": "Total Transaction Request from Western USA",
            "options": {
                "query": "index=transactions sourcetype=transaction_data method=$method$ status IN ($http_status$)\n| eval region=case(city in(\"New york\", \"Chicago\", \"Boston\", \"Washington D.C.\"), \"Eastern USA\", city in(\"San Francisco\", \"Seattle\"), \"Western USA\")\n| search region=\"Western USA\"\n| stats count",
                "queryParameters": {
                    "earliest": "$time.earliest$",
                    "latest": "$time.latest$"
                }
            },
            "type": "ds.search"
        },
        "ds_LsBC0DxN": {
            "name": "Successful Transaction Requests by Eastern US Cities",
            "options": {
                "query": "index=transactions sourcetype=transaction_data status=200\n| eval region=case(city in(\"New york\", \"Chicago\", \"Boston\", \"Washington D.C.\"), \"Eastern USA\", city in(\"San Francisco\", \"Seattle\"), \"Western USA\")\n| search region=\"Eastern USA\"\n| stats count by city",
                "queryParameters": {
                    "earliest": "$time.earliest$",
                    "latest": "$time.latest$"
                }
            },
            "type": "ds.search"
        },
        "ds_RUm6CDNi": {
            "name": "HTTP Status Codes by time",
            "options": {
                "query": "index=transactions sourcetype=transaction_data status=*\n| timechart count span=1d by status",
                "queryParameters": {
                    "earliest": "$time.earliest$",
                    "latest": "$time.latest$"
                }
            },
            "type": "ds.search"
        },
        "ds_RyPHb61Y": {
            "name": "Total Transaction Request on the Web Page",
            "options": {
                "query": "index=transactions sourcetype=transaction_data method=$method$ status IN ($http_status$)\n| stats count",
                "queryParameters": {
                    "earliest": "0",
                    "latest": ""
                }
            },
            "type": "ds.search"
        },
        "ds_ipx6q54X": {
            "name": "Successful Transaction Requests by Western US Cities",
            "options": {
                "query": "index=transactions sourcetype=transaction_data status=200\n| eval region=case(city in(\"New york\", \"Chicago\", \"Boston\", \"Washington D.C.\"), \"Eastern USA\", city in(\"San Francisco\", \"Seattle\"), \"Western USA\")\n| search region=\"Western USA\"\n| stats count by city",
                "queryParameters": {
                    "earliest": "$time.earliest$",
                    "latest": "$time.latest$"
                }
            },
            "type": "ds.search"
        },
        "ds_lS3MIUAp": {
            "name": "Successful Transaction Requests on the Web Page",
            "options": {
                "query": "index=transactions sourcetype=transaction_data status=200\n| stats count",
                "queryParameters": {
                    "earliest": "$time.earliest$",
                    "latest": "$time.latest$"
                }
            },
            "type": "ds.search"
        },
        "ds_rYDhnbyn": {
            "name": "HTTP Methods by time",
            "options": {
                "query": "index=transactions sourcetype=transaction_data method=*\n| timechart count span=1d by method",
                "queryParameters": {
                    "earliest": "$time.earliest$",
                    "latest": "$time.latest$"
                }
            },
            "type": "ds.search"
        },
        "ds_tvJlH2Gc": {
            "name": "Total Transaction Request from Eastern USA",
            "options": {
                "query": "index=transactions sourcetype=transaction_data method=$method$ status IN ($http_status$)\n| eval region=case(city in(\"New york\", \"Chicago\", \"Boston\", \"Washington D.C.\"), \"Eastern USA\", city in(\"San Francisco\", \"Seattle\"), \"Western USA\")\n| search region=\"Eastern USA\"\n| stats count",
                "queryParameters": {
                    "earliest": "$time.earliest$",
                    "latest": "$time.latest$"
                }
            },
            "type": "ds.search"
        },
        "ds_xEyrsihR": {
            "name": "Get HTTP status",
            "options": {
                "query": "index=transactions sourcetype=transaction_data status=* status!=\"status\"\n| stats count by status\n| fields status",
                "queryParameters": {
                    "earliest": "$time.earliest$",
                    "latest": "$time.latest$"
                }
            },
            "type": "ds.search"
        }
    },
